<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Access Eats"
        android:supportsRtl="true"
        android:theme="@style/Theme.AccessEatsProject"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutUs"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantOneActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantTwoActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantThreeActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantFourActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantFiveActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantSixActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantSevenActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantEightActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantNineActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantTenActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantElevenActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RestaurantTwelveActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Rating"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>